{
    /* General settings for LaTeX extension */
    "latex-workshop.latex.tools": [
        {
            "name": "latexmk",
            "command": "latexmk",
            "args": [
                "-pdflatex=\"xelatex -file-line-error -halt-on-error --shell-escape -interaction=nonstopmode -synctex=1 %O %S\"",
                "-pdf",
                "%DOCFILE%.tex"
            ]
        }
    ],
    "latex-workshop.latex.recipes": [
        {
            "name": "latexmk",
            "tools": [
                "latexmk"
            ]
        }
    ],
    "latex-workshop.latex.recipe.default": "latexmk",
    /* Fancy settings below. */
    "cSpell.ignoreWords": [
        "Bézout",
        "Gauß",
        "Vandermonde",
        "addcontentsline",
        "arraycolsep",
        "binom",
        "codomain",
        "markright",
        "mathbb",
        "mathrm",
        "notin",
        "qedhere",
        "smallmatrix",
        "subring",
        "texorpdfstring"
    ],
    "todo-tree.filtering.excludeGlobs": [
        "**/*.pdf",
        "**/*.md"
    ],
    /* Generated by "Hide Gitignored" */
    "files.exclude": {
        "**/.git": true,
